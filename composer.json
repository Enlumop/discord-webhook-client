{
    "name": "enterv/discord-webhooks",
    "description": "Professional Discord webhook client library.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "discord",
        "webhook",
        "enterv",
        "entervpl",
        "client"
    ],
    "authors": [
        {
            "name": "EnterV",
            "email": "kontakt@enterv.pl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/psr7": "^2.6",
        "guzzlehttp/guzzle": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "EnterV\\DiscordWebhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnterV\\DiscordWebhooks\\Test\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.23",
        "symfony/dotenv": "^6.3",
        "symfony/console": "^6.3",
        "phpunit/phpunit": "^10.3",
        "fakerphp/faker": "^1.23"
    },
    "scripts": {
        "analyse": "@phpstan",
        "phpstan": "vendor/bin/phpstan analyse tests src --level 9",
        "cs-fix": "vendor/bin/php-cs-fixer fix src",
        "test": "vendor/bin/phpunit tests",
        "checks": [
            "@test",
            "@phpstan"
        ]
    }
}