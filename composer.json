{
    "name": "enterv/discord-webhooks",
    "description": "Professional Discord webhook client library.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "EnterV",
            "email": "kontakt@enterv.pl"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/psr7": "^2.6",
        "guzzlehttp/guzzle": "^7.7",
        "symfony/dotenv": "^6.3",
        "enterv/vo-interfaces": "^0.0.3"
    },
    "autoload": {
        "psr-4": {
            "EnterV\\DiscordWebhooks\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "analyse": "@phpstan",
        "phpstan": "vendor/bin/phpstan analyse src --level 9"
    }
}